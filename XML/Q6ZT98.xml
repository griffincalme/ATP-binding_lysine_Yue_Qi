<scope>FUNCTION</scope>
<scope>SUBUNIT</scope>
<scope>DOMAIN</scope>
<scope>MUTAGENESIS OF ARG-106; 143-ASN--LYS-146; LYS-178; ARG-205; ARG-227; LYS-271; GLU-349; ARG-352; 385-LYS--LYS-393; 425-ARG--LYS-427 AND 477-PHE--PHE-480</scope>
